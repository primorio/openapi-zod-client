        "tanu": "^0.2.0",
        "ts-pattern": "^5.7.0",
        "whence": "^2.0.2"
    },
    "devDependencies": {
        "@changesets/changelog-github": "^0.4.8",
        "@changesets/cli": "^2.26.0",
        "@preconstruct/cli": "^2.2.1",
